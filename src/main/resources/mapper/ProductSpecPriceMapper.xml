<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ProductSpecPriceMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.domain.ProductSpecPrice">
            <id property="id" column="id" />
            <result property="productId" column="product_id" />
            <result property="specType" column="spec_type" />
            <result property="specName" column="spec_name" />
            <result property="priceAdd" column="price_add" />
            <result property="status" column="status" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,product_id,spec_type,spec_name,price_add,status,
        create_time,update_time
    </sql>
</mapper>
